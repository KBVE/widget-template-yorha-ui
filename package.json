{
  "name": "widget-template-yorha-ui",
  "version": "0.1.0",
  "private": true,
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "styled-components"
    }
  },
  "bootstrap": "yorha-ui/bootstrap.js",
  "targets": {
    "bootstrap": {
      "engines": {
        "browsers": "> 0.2%, not dead"
      },
      "includeNodeModules": true,
      "outputFormat": "global"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@parcel/config-default": "^2.9.3",
    "@parcel/core": "^2.9.3",
    "@parcel/optimizer-terser": "^2.9.3",
    "@parcel/transformer-typescript-tsc": "^2.9.3",
    "autoprefixer": "^10.4.15",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "parcel": "^2.9.3",
    "postcss": "^8.4.29",
    "process": "^0.11.10"
  },
  "scripts": {
    "kbve": "parcel build src/index.js --dist-dir yorha-ui --log-level verbose",
    "docs": "parcel build src/index.js --dist-dir docs  --log-level verbose",
    "start": "parcel src/index.html",
    "prebuild": "rm -rf dist/"
  },
  "dependencies": {
    "@nanostores/persistent": "^0.9.1",
    "@nanostores/react": "^0.7.1",
    "appwrite": "^13.0.0",
    "nanostores": "^0.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.7",
    "tailwindcss": "^3.3.3",
    "twin.macro": "^3.4.0"
  }
}
